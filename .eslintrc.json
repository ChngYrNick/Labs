{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["import"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaFeatures": {
      "globalReturn": false
    },
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": [0]
  }
}
